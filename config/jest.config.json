{
  "testEnvironment": "node",
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js",
    "!**/node_modules/**"
  ],
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "testRegex": "src/tests/*.js",
  "transform": {
    "\\.js$": "./node_modules/babel-jest"
  },
  "timers": "fake"
}

