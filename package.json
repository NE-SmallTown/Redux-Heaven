{
  "name": "redux-heaven",
  "version": "0.0.1",
  "description": "One orm library to manage our application's state combine with redux",
  "keywords": [
    "orm",
    "react",
    "redux",
    "state",
    "state-management"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0",
    "npm": "^5.0.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/babel src --out-dir lib --ignore src/examples && webpack && webpack --mode=buil",
    "flow": "flow",
    "lint": "eslint src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=development && jest --config=jest.conf.json --coverage --no-cache",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
