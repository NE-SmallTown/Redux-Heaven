{
  "testEnvironment": "node",
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 75,
      "functions": 80,
      "lines": 85,
      "statements": 85
    }
  },
  "moduleFileExtensions": [
    "js"
  ],
  "testRegex": "src/tests/.*test.js",
  "transform": {
    ".*.js": "./node_modules/babel-jest"
  }
}